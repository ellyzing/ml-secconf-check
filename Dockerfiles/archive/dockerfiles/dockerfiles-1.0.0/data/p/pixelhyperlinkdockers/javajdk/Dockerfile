# PixelHyperlink Java JDK Docker  
#  
# VERSION 1.1.0  
FROM pixelhyperlinkdockers/ubuntu:v1.8.0  
MAINTAINER Abhishek Salian <abhi@pixelhyperlink.com>  
  
LABEL Description="This image contains base ubuntu os with apache and Java
JDK."  
LABEL Vendor="PixelHyperlink"  
LABEL Version="1.1.0"  
LABEL com.pixelhyperlink.docker.javajdk.version="1.1.0"  
LABEL vendor="PixelHyperlink"  
LABEL com.pixelhyperlink.docker.javajdk.release-date="2017-07-05"  
LABEL com.pixelhyperlink.docker.javajdk.version.is-production="true"  
  
RUN apt-get update && \  
apt-get -y install default-jdk  

