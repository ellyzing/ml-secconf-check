FROM fedora:24  
MAINTAINER Yuriy Zotov <yura.zotov@gmail.com>  
  
RUN dnf -y install procps  
RUN dnf -y install psmisc  
RUN dnf -y install which  
RUN dnf -y install wget  
RUN dnf -y install iputils  
RUN dnf -y install iproute  
RUN dnf -y install tar  
RUN dnf -y install java-1.8.0-openjdk  
RUN dnf -y install java-1.8.0-openjdk-devel  
RUN dnf -y install net-tools  
RUN dnf -y install telnet  
RUN dnf -y install tcpdump  
RUN dnf -y install bind-utils  
RUN dnf -y install hostname  
  
ENV JAVA_HOME /usr/lib/jvm/jre  
  

