$ wget https://github.com/dotcloud/docker/blob/master/contrib/mkimage-rinse.sh  
$ sudo chmod u+x mkimage-rinse.sh  
$ sudo ./mkimage-rinse.sh flores314/centos65 centos-6
http://centos.mirror.constant.com/6.5/os/x86_64/Packages/  
  
DISTRIB_ID=CentOS  
DISTRIB_RELEASE=6.5  
DISTRIB_CODENAME=minimal  
DISTRIB_DESCRIPTION="CentOS 6.5"  
  
$ docker push flores314/centos65

